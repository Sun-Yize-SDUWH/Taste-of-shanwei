page {
  background-color: #fff;
  font-family: "Helvetica Neue",
                 "Hiragino Sans GB",
                 "Microsoft YaHei",
                 "\9ED1\4F53",
                 Arial,
                 sans-serif;
  font-size: 32rpx;
}

.iconStyle{
  margin-right: 10rpx;
  position: absolute;
  left: 17px;
  top: 17px;
}

.selected{
  margin-bottom: -20rpx;
}

.navigator-hover {
  background: none;
}

.search {
  padding: 20rpx;
}

.search-text {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 50rpx;
  color: #aaa;
  text-align: center;
  border: 1px solid #ddd;
  border-radius: 20rpx;
}

.search-text icon {
  margin-right: 10rpx;
}

swiper {
  height: 300rpx;
}

swiper-item image {
  width: 100%;
  height: 100%;
}

.select-title {
  width: 100%;
  padding: 30rpx 0;
  height: 40rpx;
  color: #ab956d;
  text-align: center;
}

.select-top-small {
  margin-left: 3rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  width: 49%;
  height: 125rpx;
  float: left;
  border: 1rpx solid gainsboro;
}

.select-bottom, .select-top {
  width: 100%;
  height: 375rpx;
}

.select-top-small image, .select-bottom image, .select-top-small navigator,
.select-bottom navigator {
  display: block;
  width: 100%;
  height: 100%;
}

.newest-title {
  width: 100%;
  padding: 30rpx 0;
  height: 40rpx;
  color: #ab956d;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}

.newest-box {
  padding: 0 40rpx;
}

.newest-box .newest-list {
  display: inline-block;
  width: 30%;
  height: 200rpx;
  margin: 0 20rpx 8rpx 8rpx;
  border-radius: 10px;
  text-align: center;
  background: #f5f6f5;
}

.newest-box .newest-list:nth-child(2n) {
  margin-right: 0;
}

.newest-box .newest-list image {
  width: 175rpx;
  height: 175rpx;
  margin: 20rpx 0 10rpx;
}

.newest-box .newest-list .newest-text {
  font-size: 32rpx;
}

.nav {
  height: 100%;
  width: 100%;
}
