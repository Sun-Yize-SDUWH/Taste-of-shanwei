<view class="main">
  <view class="search">
    <navigator url="../search/search">
      <view class="search-text">
        <icon class="iconStyle" type="search" size="14"></icon>
        搜索商家信息
      </view>
    </navigator>
  </view>
  <view class="selected">
    <view class="select-title">
      <text>食堂快速通道</text>
    </view>
    <view class="select-top">
      <view class="select-top-small">
        <navigator url="../dininghall/xinyuan/dininghall-floor" class="nav">
          <image src="../image/d1.jpg"></image>
        </navigator>
      </view>
      <view class="select-top-small">
        <navigator url="../dininghall/huiyuan/dininghall-floor" class="nav">
          <image src="../image/d2.jpg"></image>
        </navigator>
      </view>
      <view class="select-top-small">
        <navigator url="../dininghall/taiyuan/dininghall-floor" class="nav">
          <image src="../image/d3.jpg"></image>
        </navigator>
      </view>
      <view class="select-top-small">
        <navigator url="../dininghall/queyuan/dininghall-floor" class="nav">
          <image src="../image/d4.jpg"></image>
        </navigator>
      </view>
    </view>
  </view>

<view class="select-title">
      <text>美食推荐</text>
    </view>
   <swiper indicator-dots="true" autoplay="true" interval="{{interval}}" duration="{{duration}}" circular="true">
    <block wx:for="{{imgUrls}}" wx:key="index">
      <swiper-item>
        <image src="{{item}}" bindtap='click_swiper' class="slide-image" width="100%" />
      </swiper-item>
    </block>
  </swiper>


  <view class="newest">
    <view class="newest-title">
      <text>餐品分类</text>
    </view>
    <view class="newest-box">
      <view class="newest-list">
        <navigator url="details/details">
          <image src="../image/f1.jpg" class="separate" style="height:160rpx;width:160rpx"></image>
          <view class="newest-text">
            <text>麻辣香锅</text>
          </view>

        </navigator>
      </view>
      <view class="newest-list">
        <navigator url="details/details">
          <image src="../image/f2.jpg" class="separate" style="height:160rpx;width:160rpx"></image>
          <view class="newest-text">
            <text>甜品饮品</text>
          </view>

        </navigator>
      </view>
      <view class="newest-list">
        <navigator url="details/details">
          <image src="../image/f3.jpg" class="separate" style="height:160rpx;width:160rpx"></image>
          <view class="newest-text">
            <text>健康早餐</text>
          </view>

        </navigator>
      </view>
      <view class="newest-list">
        <navigator url="details/details">
          <image src="../image/f4.jpg" class="separate" style="height:160rpx;width:160rpx"></image>
          <view class="newest-text">
            <text>快餐</text>
          </view>

        </navigator>
      </view>
      <view class="newest-list">
        <navigator url="details/details">
          <image src="../image/f5.jpg" class="separate" style="height:160rpx;width:160rpx"></image>
          <view class="newest-text">
            <text>面食</text>
          </view>

        </navigator>
      </view>

      <view class="newest-list">
        <navigator url="details/details">
          <image src="../image/f6.jpg" class="separate" style="height:160rpx;width:160rpx"></image>
          <view class="newest-text">
            <text>盘餐</text>
          </view>

        </navigator>
      </view>
    </view>
  </view>




</view>