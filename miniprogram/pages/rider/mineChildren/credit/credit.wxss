Page {
  background-color: rgba(218, 210, 210, 0.185);
}

.header{
  display: flex;
  justify-content: center;
  align-items: center;
  padding-top: 70rpx;
  padding-bottom: 40rpx;
  width: 100%;
  background-color:#23EBB9;
  color: #ffffff;
}

.header-center{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.record{
  height: 74rpx;
  line-height: 74rpx;
  padding: 0 60rpx;
  text-align: center;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fed9c2;
}

.footer-item{
  width: 710rpx;
  padding: 10px 20rpx
}

.item-top{
  display: flex;
  margin-top: 10px
}

.item-top-left{
  width: 70%
}

.item-top-right{
  width: 30%;
  text-align: right
}

.item-bottom-left{
  width: 50%
}

.item-bottom-right{
  width: 50%;
  text-align: right
}

